{
  "rewrites": [
    {
      "source": "/admin/:path*",
      "destination": "/my-tina-app/static/admin/index.html"
    },
    {
      "source": "/tina/admin/:path*",
      "destination": "/my-tina-app/static/admin/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}